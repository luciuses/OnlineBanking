<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <script src="~/Scripts/jquery-2.1.1.js"></script>
    @*<script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>*@
    <link rel="stylesheet/less" type="text/css" href="~/Content/MainStyle.less">
    <script src="~/Scripts/less-1.7.0.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.qtip-1.0.0-rc3.min.js"></script>
   
</head>
<body>

    <header >
        <div id="logo" onclick="location.href = '@(Url.Action("List","Manager"))'">
            <img src="~/Content/Image/logo.png" alt="" id="logo-image" />
            <div id="logo-text">Welcome to online banking system</div>
        </div>
    </header>
   @Html.ValidationSummary(true)
        @RenderBody()

    <footer>
       <div id="footer-text">@("@") @(DateTime.Today.Year.ToString()) Online Banking. All rights reserved.</div>
    </footer>
    
</body>
</html>